<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.electronicstudentid">

    <uses-permission android:name="android.permission.NFC" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-feature
        android:name="android.hardware.nfc"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/student"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".MainViewActivity"  android:theme="@style/AppTheme.Dark">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>
        <activity android:name=".LoginRegister.LoginActivity" android:theme="@style/AppTheme.Dark">

        </activity>
        <activity android:name=".LoginRegister.SignUpActivity" android:theme="@style/AppTheme.Dark" />
        <activity android:name=".Nfc.NfcActivity" android:theme="@style/AppTheme.Dark" />
    </application>
</manifest>